   "source": [
    "class Solution:\n",
    "    def grayCode(self, n: int) -> list:\n",
    "        if n == 0:\n",
    "            return [0]\n",
    "        else:\n",
    "            left = self.grayCode(n-1)\n",
    "            return left + [l+(1<<(n-1)) for l in left[::-1]]"
   ]
  },
